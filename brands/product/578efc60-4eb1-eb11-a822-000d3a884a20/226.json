        "fees": [
            {
                "name": "Additional Valuation",
                "feeType": "EVENT",
                "amount": "150.00",
                "currency": "AUD",
                "additionalInfo": "The establishment fee covers a standard loan, which includes one valuation. This fee applies for any additional standard residential valuation required."
            },
            {
                "name": "Break Fee",
                "feeType": "VARIABLE",
                "currency": "AUD",
                "additionalInfo": "You may be required to pay a break fee if you pay your fixed loan off in full, make an early repayment or make certain other changes to your loan during any fixed period. The fee is calculated at the time of the event and is specifically related to your individual loan. More information is found in the loan fees and charges document.",
                "additionalInfoUri": "https://cdn.bfldr.com/1R31Y8GR/as/5pxcb6tz9n83psz2k2knqtk/Loan_fees_schedule"
            },
            {
                "name": "Home Loan Establishment Fee - Existing Mortgage",
                "feeType": "UPFRONT",
                "amount": "295.00",
                "currency": "AUD",
                "additionalInfo": "Payable for all requests to increase a current loan or for an additional loan where Bank Australia already holds a registered mortgage."
            },
            {
                "name": "Home Loan Establishment Fee - New mortgage",
                "feeType": "UPFRONT",
                "amount": "595.00",
                "currency": "AUD",
                "additionalInfo": "Payable for all mortgage secured loans to cover Bank Australia administration costs including preparing security documents."
            },
            {
                "name": "Loan Variation Fee",
                "feeType": "EVENT",
                "amount": "150.00",
                "currency": "AUD",
                "additionalInfo": "Payable when transferring from a home loan product to a different home loan product. For example, transferring a variable rate product to a fixed rate product."
            },
            {
                "name": "Mortgage Discharge Fee",
                "feeType": "EVENT",
                "amount": "350.00",
                "currency": "AUD",
                "additionalInfo": "Payable when you discharge the mortgage. Statutory government registration fees will still apply"
            },
            {
                "name": "Portability Fee- Existing Mortgage",
                "feeType": "EVENT",
                "amount": "150.00",
                "currency": "AUD",
                "additionalInfo": "Payable when a property securing a loan is replaced with a property that is already registered with a Bank Australia mortgage."
            },
            {
                "name": "Portability Fee- New Mortgage",
                "feeType": "EVENT",
                "amount": "500.00",
                "currency": "AUD",
                "additionalInfo": "Payable when you request that a property securing a loan is replaced with a property that is a new security to Bank Australia."
            },
            {
                "name": "Valuation Administration Fee",
                "feeType": "EVENT",
                "amount": "250.00",
                "currency": "AUD",
                "additionalInfo": "If you request a loan variation, Bank Australia may need to revalue your property before approving the request."
            },
            {
                "name": "Rate Lock",
                "feeType": "VARIABLE",
                "transactionRate": "0.001",
                "currency": "AUD",
                "additionalInfo": "Payable when you choose to lock in the fixed rate at application. 0.10% of the total fixed loan amount for approved home loan applications (minimum fee $50). This applies to 1, 2, 3 and 5 year Fixed Rate home loans only and is a non-refundable fee."
            }
        ],

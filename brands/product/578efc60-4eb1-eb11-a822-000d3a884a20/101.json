        "features": [
            {
                "featureType": "BILL_PAYMENT",
                "additionalValue": "BPAY"
            },
            {
                "featureType": "CARD_ACCESS",
                "additionalValue": "Visa Debit Card",
                "additionalInfo": "Use instore, online and withdraw local currency at millions of locations, everywhere Visa is accepted. Cards are enabled to support diverse needs with raised bumps on the left hand-side on the front face of the card."
            },
            {
                "featureType": "DIGITAL_BANKING",
                "additionalInfo": "You can self-serve your day-to-day banking needs via Bank Australia's App or Internet Banking."
            },
            {
                "featureType": "DIGITAL_WALLET",
                "additionalValue": "Google, Samsung and Apple Pay available",
                "additionalInfo": "Leave your wallet behind and pay for the things you need with Google Pay/ Samsung Pay/ Apple Pay."
            },
            {
                "featureType": "NOTIFICATIONS",
                "additionalValue": "Card and Account Notifications",
                "additionalInfo": "Notifications can be enabled in the App or through Internet Banking, allowing you to be alerted either by email, sms or through the App about your card/account activity."
            },
            {
                "featureType": "NPP_ENABLED"
            },
            {
                "featureType": "NPP_PAYID",
                "additionalInfo": "With PayID you can forget your BSB and account number - simply use your mobile, email or ABN to receive payments. This account can be linked to a PayID."
            },
            {
                "featureType": "OTHER",
                "additionalInfo": "Card Controls: Decide the features you want enabled on your card including card lock, digital wallet, pay wave, international transactions, ATM withdrawals or online/in store transactions and simply switch them on or off from your App or Internet Banking."
            },
            {
                "featureType": "OTHER",
                "additionalInfo": "Round Up: When you make a transaction with your Visa debit card, you can decide whether you want the amount to be rounded to the nearest whole dollar amount, with the round up being credited from this account into your nominated Savings accountor Credit Card*."
            },
            {
                "featureType": "UNLIMITED_TXNS",
                "additionalInfo": "There is no limit to the number of online, EFTPOS, ATM or staff assisted transactions you can make."
            }
        ],

        "features": [
            {
                "featureType": "DIGITAL_BANKING",
                "additionalInfo": "You can self-serve your day-to-day banking needs via Bank Australia's App, Internet Banking or Phone Banking."
            },
            {
                "featureType": "OTHER",
                "additionalValue": "Flexible Repayments",
                "additionalInfo": "Make your loan repayments weekly, fortnightly or monthly"
            },
            {
                "featureType": "OTHER",
                "additionalValue": "Parental Pause",
                "additionalInfo": "Welcoming a new family member to the family can place a pretty big strain on finances so we offer you the option of pausing your home loan repayments to give you some breathing space.  This allows you to take a break from payments for three months, or halve them for six. Conditions apply",
                "additionalInfoUri": "https://www.bankaust.com.au/personal/borrow/home-loan-hub/buying/parentalpause"
            },
            {
                "featureType": "OTHER",
                "additionalValue": "Eco-Pause",
                "additionalInfo": "Our Eco-Pause lets customers take a break from their mortgage repayments and redirect their money to eco upgrades such as solar panels, batteries and water tanks. With an eco-pause you can pause your repayments for three months or halve them for six. Conditions apply.",
                "additionalInfoUri": "https://www.bankaust.com.au/personal/borrow/home-loan-hub/buying/ecopause"
            },
            {
                "featureType": "OTHER",
                "additionalValue": "Construction Loan Offset",
                "additionalInfo": "We protect an equivalent amount of land on the Bank Australia Conservation Reserve as is being constructed to help offset the environmental impact of land clearing"
            }
        ],
        "eligibility": [
            {
                "eligibilityType": "NATURAL_PERSON",
                "additionalValue": "Individuals only",
                "additionalInfo": "Not available for non-personal borrowers."
            },
            {
                "eligibilityType": "MIN_AGE",
                "additionalValue": "18",
                "additionalInfo": "Individuals (natural persons) must have the legal capacity to enter into the loan contract, provide acceptable identification documentation and be 18 years or older."
            },
            {
                "eligibilityType": "RESIDENCY_STATUS",
                "additionalValue": "AU",
                "additionalInfo": "Australian citizen or permanent resident of Australia"
            },
            {
                "eligibilityType": "OTHER",
                "additionalInfo": "Applications for loans are subject to the Bank's lending criteria."
            },
            {
                "eligibilityType": "BUSINESS",
                "additionalValue": "Non-individual borrowers",
                "additionalInfo": "Includes Non-trading Companies and/or Family/Unit/Hybrid Trusts"
            }
        ],
        "fees": [
            {
                "name": "Additional Valuation",
                "feeType": "EVENT",
                "amount": "150.00",
                "additionalInfo": "The establishment fee covers a standard loan, which includes one valuation. This fee applies for any additional standard residential valuation required."
            },
            {
                "name": "Break Fee",
                "feeType": "EVENT",
                "amount": "0.00",
                "additionalInfo": "You may be required to pay a break fee if you pay your fixed loan off in full, make an early repayment or make certain other changes to your loan during any fixed period. The fee is calculated at the time of the event and is specifically related to your individual loan. More information is found in the loan fees and charges document."
            },
            {
                "name": "Home Loan Establishment Fee - Existing Mortgage",
                "feeType": "UPFRONT",
                "amount": "295.00",
                "additionalInfo": "Payable for all requests to increase a current loan or for an additional loan where Bank Australia already holds a registered mortgage."
            },
            {
                "name": "Home Loan Establishment Fee - New mortgage",
                "feeType": "UPFRONT",
                "amount": "595.00",
                "additionalInfo": "Payable for all mortgage secured loans to cover Bank Australia administration costs including preparing security documents."
            },
            {
                "name": "Loan Variation Fee",
                "feeType": "EVENT",
                "amount": "150.00",
                "additionalInfo": "Payable when transferring from a home loan product to a different home loan product. For example, transferring a variable rate product to a fixed rate product."
            },
            {
                "name": "Mortgage Discharge Fee",
                "feeType": "EVENT",
                "amount": "250.00",
                "additionalInfo": "Payable when you request Bank Australia to arrange for the registration of discharge or Bank Australia is required to attend settlement. It covers preparation of the discharge of mortgage form. Statutory government registration fees will still apply.  Additional fees may apply and should be paid direct to our agent if settlement is requested in a location other than a Bank Australia office."
            },
            {
                "name": "Portability Fee- Existing Mortgage",
                "feeType": "EVENT",
                "amount": "150.00",
                "additionalInfo": "Payable when a property securing a loan is replaced with a property that is already registered with a Bank Australia mortgage."
            },
            {
                "name": "Portability Fee- New Mortgage",
                "feeType": "EVENT",
                "amount": "500.00",
                "additionalInfo": "Payable when you request that a property securing a loan is replaced with a property that is a new security to Bank Australia."
            },
            {
                "name": "Valuation Administration Fee",
                "feeType": "EVENT",
                "amount": "250.00",
                "additionalInfo": "If you request a loan variation, Bank Australia may need to revalue your property before approving the request."
            },
            {
                "name": "Rate Lock Fee",
                "feeType": "EVENT",
                "balanceRate": "0.0010",
                "currency": "AUD",
                "additionalInfo": "Payable only if you request to lock in your fixed rate interest rates and Bank Australia accepts your application. This applies to 1, 2, 3 and 5 year Fixed Rate home loans only and is a non-refundable fee. 0.1% of approved loan amount, minimum $50"
            }
        ],

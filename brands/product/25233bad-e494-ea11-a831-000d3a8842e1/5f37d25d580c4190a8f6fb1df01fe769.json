            },
            {
                "eligibilityType": "RESIDENCY_STATUS",
                "additionalInfo": "Individuals must either be living in Australia as an Australian citizen, New Zealand citizen, Australian permanent resident or specified temporary Australian resident; or be an overseas citizen co-borrowing with an Australian citizen or Australian permanent resident or be living overseas as an Australian citizen or Australian permanent resident."
            }
        ],

        "fees": [
            {
                "name": "Excess Cheque Fee",
                "feeType": "EVENT",
                "amount": "8.00",
                "additionalInfo": "You can request 4 bank cheques for free. An extra 5 free bank cheques are available for progress payments on construction loans. After you have used the free cheques, a fee is payable for each bank cheque requested."
            },
            {
                "name": "Arrears Administration Fee",
                "feeType": "EVENT",
                "amount": "20.00",
                "additionalInfo": "The bank at its discretion will charge either an Arrears Administration Fee or Default Interest if an event of default in payment occurs."
            },
            {
                "name": "Copy of Documents Fee",
                "feeType": "EVENT",
                "amount": "35.00",
                "additionalInfo": "Payable if we are required to obtain a copy of a document, including Mortgage or title search."
            },
            {
                "name": "Additional Valuation Fee",
                "feeType": "EVENT",
                "amount": "150.00",
                "additionalInfo": "The first valuation assessment for each loan application is provided by the Bank. If any additional valuation assessments that involve a physical inspection are obtained by us for assessing the loan security, than this fee will apply."
            },
            {
                "name": "Guarantor Fee",
                "feeType": "EVENT",
                "amount": "200.00",
                "additionalInfo": "Applies if a guarantor is included in the loan. Fee is per guarantor."
            },
            {
                "name": "Renegotiation Fee",
                "feeType": "EVENT",
                "amount": "300.00",
                "additionalInfo": "Payable when you request a change to the Credit Contract or Security and this change is completed."
            },
            {
                "name": "Loan Finalisation Fee",
                "feeType": "EVENT",
                "amount": "350.00",
                "additionalInfo": "Applies when a loan is paid out in full, This includes settlements, refinances and payouts."
            },
            {
                "name": "Default Interest",
                "feeType": "EVENT",
                "balanceRate": "0.02",
                "additionalInfo": "The bank at its discretion will charge either an Arrears Administration Fee or Default Interest if an event of default in payment occurs."
            },
            {
                "name": "Monthly Account Keeping Fee",
                "feeType": "PERIODIC",
                "amount": "10.00",
                "additionalInfo": "Please refer to your Credit Contract for detail on when Ongoing Fees are charged."
            },
            {
                "name": "Early Payment Interest Adjustment (EPIA)",
                "feeType": "VARIABLE",
                "additionalInfo": "Charged when you, (a) Payout the loan during a Fixed Rate period (b) Pay more than the agreed monthly amount (c) Vary or break out of a Fixed Rate period."
            },
            {
                "name": "Establishment Fee",
                "feeType": "UPFRONT",
                "amount": "600.00",
                "additionalInfo": "Applies to loans where borrowings are less than 90% of the property value"
            },
            {
                "name": "Establishment Fee",
                "feeType": "UPFRONT",
                "amount": "990.00",

            {
                "name": "Guarantor Fee",
                "feeType": "EVENT",
                "amount": "200.00",
                "additionalInfo": "Applies if a guarantor is included in the loan. Fee is per guarantor."
            },
            {
                "name": "Renegotiation Fee",
                "feeType": "EVENT",
                "amount": "300.00",
                "additionalInfo": "Payable when you request a change to the Credit Contract or Security and this change is completed."
            },
            {
                "name": "Loan Finalisation Fee",
                "feeType": "EVENT",
                "amount": "350.00",
                "additionalInfo": "Applies when a loan is paid out in full, This includes settlements, refinances and payouts."
            },
            {
                "name": "Default Interest",
                "feeType": "EVENT",
